   "source": [
    "# Interactive mammogram generation\n",
    "def generate_mammogram(view, laterality, age_group, cancer, false_positive, num_samples=4):\n",
    "    \"\"\"\n",
    "    Generate mammograms with specified conditions\n",
    "    \"\"\"\n",
    "    # Convert parameters to model format\n",
    "    view_map = {'CC': 0, 'MLO': 1}\n",
    "    laterality_map = {'Left': 0, 'Right': 1}\n",
    "    age_map = {'Young (40-50)': 0, 'Middle (50-60)': 1, 'Older (60-70)': 2, 'Senior (70+)': 3}\n",
    "    \n",
    "    conditions = {\n",
    "        'view': torch.tensor([view_map[view]] * num_samples, device=device),\n",
    "        'laterality': torch.tensor([laterality_map[laterality]] * num_samples, device=device),\n",
    "        'age_bin': torch.tensor([age_map[age_group]] * num_samples, device=device),\n",
    "        'cancer': torch.tensor([int(cancer)] * num_samples, device=device),\n",
    "        'false_positive': torch.tensor([int(false_positive)] * num_samples, device=device)\n",
    "    }\n",
    "    \n",
    "    # Generate samples\n",
    "    with torch.no_grad():\n",
    "        generated = model.sample(conditions, batch_size=num_samples)\n",
    "    \n",
    "    # Plot results\n",
    "    fig, axes = plt.subplots(1, num_samples, figsize=(16, 4))\n",
    "    if num_samples == 1:\n",
    "        axes = [axes]\n",
    "    \n",
    "    for i in range(num_samples):\n",
    "        axes[i].imshow(generated[i, 0].cpu().numpy(), cmap='gray')\n",
    "        axes[i].set_title(f'Sample {i+1}')\n",
    "        axes[i].axis('off')\n",
    "    \n",
    "    # Add overall title with conditions\n",
    "    cancer_status = 'Cancer' if cancer else 'Normal'\n",
    "    fp_status = 'False Positive' if false_positive else 'True Negative'\n",
    "    title = f'{view} View, {laterality} Breast, {age_group}, {cancer_status}'\n",
    "    if not cancer:\n",
    "        title += f', {fp_status}'\n",
    "    \n",
    "    plt.suptitle(title, fontsize=14)\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "# Create interactive widgets\n",
    "view_widget = widgets.Dropdown(\n",
    "    options=['CC', 'MLO'],\n",
    "    value='CC',\n",
    "    description='View:'\n",
    ")\n",
    "\n",
    "laterality_widget = widgets.Dropdown(\n",
    "    options=['Left', 'Right'],\n",
    "    value='Left',\n",
    "    description='Laterality:'\n",
    ")\n",
    "\n",
    "age_widget = widgets.Dropdown(\n",
    "    options=['Young (40-50)', 'Middle (50-60)', 'Older (60-70)', 'Senior (70+)'],\n",
    "    value='Middle (50-60)',\n",
    "    description='Age Group:'\n",
    ")\n",
    "\n",
    "cancer_widget = widgets.Checkbox(\n",
    "    value=False,\n",
    "    description='Cancer Present'\n",
    ")\n",
    "\n",
    "fp_widget = widgets.Checkbox(\n",
    "    value=False,\n",
    "    description='False Positive'\n",
    ")\n",
    "\n",
    "samples_widget = widgets.IntSlider(\n",
    "    value=4,\n",
    "    min=1,\n",
    "    max=8,\n",
    "    step=1,\n",
    "    description='Samples:'\n",
    ")\n",
    "\n",
    "# Create interactive interface\n",
    "interactive_plot = interactive(\n",
    "    generate_mammogram,\n",
    "    view=view_widget,\n",
    "    laterality=laterality_widget,\n",
    "    age_group=age_widget,\n",
    "    cancer=cancer_widget,\n",
    "    false_positive=fp_widget,\n",
    "    num_samples=samples_widget\n",
    ")\n",
    "\n",
    "display(interactive_plot)"
   ]
  },
