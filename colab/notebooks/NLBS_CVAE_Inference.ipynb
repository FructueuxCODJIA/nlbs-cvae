    "id": "load_model"
   },
   "outputs": [],
   "source": [
    "# Load pre-trained model\n",
    "# You can either:\n",
    "# 1. Load from Google Drive (if you have a trained model)\n",
    "# 2. Download from a public URL\n",
    "# 3. Use a demo model\n",
    "\n",
    "# Option 1: Load from Google Drive\n",
    "model_path = '/content/drive/MyDrive/NLBS_CVAE_Results_*/checkpoints/best_model.pth'\n",
    "\n",
    "# Find the most recent model\n",
    "import glob\n",
    "model_files = glob.glob(model_path)\n",
    "\n",
    "if model_files:\n",
    "    latest_model = max(model_files, key=lambda x: Path(x).stat().st_mtime)\n",
    "    print(f\"Loading model from: {latest_model}\")\n",
    "    \n",
    "    # Load configuration (assuming it's in the same results folder)\n",
    "    config_path = Path(latest_model).parent.parent / 'config.yaml'\n",
    "    if config_path.exists():\n",
    "        with open(config_path, 'r') as f:\n",
    "            config = yaml.safe_load(f)\n",
    "    else:\n",
    "        # Use default config\n",
    "        with open('/content/nlbs-cvae/colab/configs/colab_training_config.yaml', 'r') as f:\n",
    "            config = yaml.safe_load(f)\n",
    "    \n",
    "    # Create model\n",
    "    model = ConditionalVAE(\n",
    "        in_channels=config['data']['channels'],\n",
    "        image_size=config['data']['resolution'],\n",
    "        latent_dim=config['model']['latent_dim'],\n",
    "        condition_embed_dim=config['model']['condition_embed_dim'],\n",
    "        encoder_channels=config['model']['encoder']['channels'],\n",
    "        decoder_channels=config['model']['decoder']['channels']\n",
    "    ).to(device)\n",
    "    \n",
    "    # Load weights\n",
    "    checkpoint = torch.load(latest_model, map_location=device)\n",
    "    model.load_state_dict(checkpoint['model_state_dict'])\n",
    "    model.eval()\n",
    "    \n",
    "    print(f\"‚úÖ Model loaded successfully!\")\n",
    "    print(f\"Trained for {checkpoint['epoch']} epochs\")\n",
    "    print(f\"Best loss: {checkpoint['loss']:.4f}\")\n",
    "    \n",
    "else:\n",
    "    print(\"‚ùå No pre-trained model found.\")\n",
    "    print(\"Please run the training notebook first or provide a model path.\")\n",
    "    \n",
    "    # Create a dummy model for demonstration\n",
    "    print(\"Creating demo model...\")\n",
    "    with open('/content/nlbs-cvae/colab/configs/colab_training_config.yaml', 'r') as f:\n",
    "        config = yaml.safe_load(f)\n",
    "    \n",
    "    model = ConditionalVAE(\n",
    "        in_channels=config['data']['channels'],\n",
    "        image_size=config['data']['resolution'],\n",
    "        latent_dim=config['model']['latent_dim'],\n",
    "        condition_embed_dim=config['model']['condition_embed_dim'],\n",
    "        encoder_channels=config['model']['encoder']['channels'],\n",
    "        decoder_channels=config['model']['decoder']['channels']\n",
    "    ).to(device)\n",
    "    \n",
    "    print(\"‚ö†Ô∏è Using untrained model for demonstration\")"
   ]
