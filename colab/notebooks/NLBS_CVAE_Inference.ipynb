    "# Batch generation for different conditions\n",
    "def generate_condition_matrix():\n",
    "    \"\"\"\n",
    "    Generate a matrix showing different condition combinations\n",
    "    \"\"\"\n",
    "    views = ['CC', 'MLO']\n",
    "    lateralities = ['Left', 'Right']\n",
    "    cancer_states = [False, True]\n",
    "    \n",
    "    fig, axes = plt.subplots(len(views) * len(cancer_states), len(lateralities), figsize=(8, 12))\n",
    "    \n",
    "    row = 0\n",
    "    for view_idx, view in enumerate(views):\n",
    "        for cancer_idx, cancer in enumerate(cancer_states):\n",
    "            for lat_idx, laterality in enumerate(lateralities):\n",
    "                # Create conditions\n",
    "                conditions = {\n",
    "                    'view': torch.tensor([view_idx], device=device),\n",
    "                    'laterality': torch.tensor([lat_idx], device=device),\n",
    "                    'age_bin': torch.tensor([1], device=device),  # Middle age\n",
    "                    'cancer': torch.tensor([int(cancer)], device=device),\n",
    "                    'false_positive': torch.tensor([0], device=device)\n",
    "                }\n",
    "                \n",
    "                # Generate\n",
    "                with torch.no_grad():\n",
    "                    generated = model.sample(conditions, batch_size=1)\n",
    "                \n",
    "                # Plot\n",
    "                axes[row, lat_idx].imshow(generated[0, 0].cpu().numpy(), cmap='gray')\n",
    "                \n",
    "                # Title\n",
    "                cancer_status = 'Cancer' if cancer else 'Normal'\n",
    "                title = f'{view} {laterality}\\n{cancer_status}'\n",
    "                axes[row, lat_idx].set_title(title, fontsize=10)\n",
    "                axes[row, lat_idx].axis('off')\n",
    "            \n",
    "            row += 1\n",
    "    \n",
    "    plt.suptitle('Condition Matrix: View √ó Laterality √ó Cancer Status', fontsize=14)\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "# Generate condition matrix\n",
    "print(\"Generating condition matrix...\")\n",
    "generate_condition_matrix()"
   ]
