    "# Latent space exploration\n",
    "def explore_latent_space(dim1, dim2, range_val=3.0, grid_size=5):\n",
    "    \"\"\"\n",
    "    Explore the latent space by varying two dimensions\n",
    "    \"\"\"\n",
    "    # Fixed conditions for consistency\n",
    "    conditions = {\n",
    "        'view': torch.tensor([0], device=device),  # CC\n",
    "        'laterality': torch.tensor([0], device=device),  # Left\n",
    "        'age_bin': torch.tensor([1], device=device),  # Middle age\n",
    "        'cancer': torch.tensor([0], device=device),  # No cancer\n",
    "        'false_positive': torch.tensor([0], device=device)  # Not false positive\n",
    "    }\n",
    "    \n",
    "    # Create grid of latent codes\n",
    "    latent_dim = config['model']['latent_dim']\n",
    "    \n",
    "    fig, axes = plt.subplots(grid_size, grid_size, figsize=(12, 12))\n",
    "    \n",
    "    values = np.linspace(-range_val, range_val, grid_size)\n",
    "    \n",
    "    for i, val1 in enumerate(values):\n",
    "        for j, val2 in enumerate(values):\n",
    "            # Create latent code\n",
    "            z = torch.zeros(1, latent_dim, device=device)\n",
    "            z[0, dim1] = val1\n",
    "            z[0, dim2] = val2\n",
    "            \n",
    "            # Generate image\n",
    "            with torch.no_grad():\n",
    "                output = model.decode(z, conditions)\n",
    "                generated = output['reconstruction']\n",
    "            \n",
    "            # Plot\n",
    "            axes[i, j].imshow(generated[0, 0].cpu().numpy(), cmap='gray')\n",
    "            axes[i, j].set_title(f'({val1:.1f}, {val2:.1f})', fontsize=8)\n",
    "            axes[i, j].axis('off')\n",
    "    \n",
    "    plt.suptitle(f'Latent Space Exploration (Dimensions {dim1} vs {dim2})', fontsize=14)\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "# Interactive latent exploration\n",
    "dim1_widget = widgets.IntSlider(\n",
    "    value=0,\n",
    "    min=0,\n",
    "    max=config['model']['latent_dim']-1,\n",
    "    description='Dimension 1:'\n",
    ")\n",
    "\n",
    "dim2_widget = widgets.IntSlider(\n",
    "    value=1,\n",
    "    min=0,\n",
    "    max=config['model']['latent_dim']-1,\n",
    "    description='Dimension 2:'\n",
    ")\n",
    "\n",
    "range_widget = widgets.FloatSlider(\n",
    "    value=2.0,\n",
    "    min=0.5,\n",
    "    max=5.0,\n",
    "    step=0.5,\n",
    "    description='Range:'\n",
    ")\n",
    "\n",
    "grid_widget = widgets.IntSlider(\n",
    "    value=5,\n",
    "    min=3,\n",
    "    max=7,\n",
    "    description='Grid Size:'\n",
    ")\n",
    "\n",
    "latent_interactive = interactive(\n",
    "    explore_latent_space,\n",
    "    dim1=dim1_widget,\n",
    "    dim2=dim2_widget,\n",
    "    range_val=range_widget,\n",
    "    grid_size=grid_widget\n",
    ")\n",
    "\n",
    "display(latent_interactive)"
   ]
  },
