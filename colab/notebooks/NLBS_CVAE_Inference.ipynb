    "# Save generated images\n",
    "def save_generated_samples(num_samples=20):\n",
    "    \"\"\"\n",
    "    Generate and save a collection of diverse samples\n",
    "    \"\"\"\n",
    "    print(f\"Generating {num_samples} diverse samples...\")\n",
    "    \n",
    "    # Create output directory\n",
    "    output_dir = Path('/content/generated_samples')\n",
    "    output_dir.mkdir(exist_ok=True)\n",
    "    \n",
    "    # Generate diverse conditions\n",
    "    samples_data = []\n",
    "    \n",
    "    for i in range(num_samples):\n",
    "        # Random conditions\n",
    "        view = np.random.choice([0, 1])\n",
    "        laterality = np.random.choice([0, 1])\n",
    "        age_bin = np.random.choice([0, 1, 2, 3])\n",
    "        cancer = np.random.choice([0, 1], p=[0.7, 0.3])\n",
    "        false_positive = np.random.choice([0, 1], p=[0.9, 0.1])\n",
    "        \n",
    "        conditions = {\n",
    "            'view': torch.tensor([view], device=device),\n",
    "            'laterality': torch.tensor([laterality], device=device),\n",
    "            'age_bin': torch.tensor([age_bin], device=device),\n",
    "            'cancer': torch.tensor([cancer], device=device),\n",
    "            'false_positive': torch.tensor([false_positive], device=device)\n",
    "        }\n",
    "        \n",
    "        # Generate\n",
    "        with torch.no_grad():\n",
    "            generated = model.sample(conditions, batch_size=1)\n",
    "        \n",
    "        # Save image\n",
    "        image = generated[0, 0].cpu().numpy()\n",
    "        \n",
    "        # Create filename with conditions\n",
    "        view_name = 'CC' if view == 0 else 'MLO'\n",
    "        lat_name = 'L' if laterality == 0 else 'R'\n",
    "        cancer_name = 'cancer' if cancer == 1 else 'normal'\n",
    "        \n",
    "        filename = f'sample_{i:03d}_{view_name}_{lat_name}_age{age_bin}_{cancer_name}.png'\n",
    "        \n",
    "        plt.figure(figsize=(6, 6))\n",
    "        plt.imshow(image, cmap='gray')\n",
    "        plt.title(f'{view_name} View, {lat_name} Breast, Age Group {age_bin}, {cancer_name.title()}')\n",
    "        plt.axis('off')\n",
    "        plt.savefig(output_dir / filename, dpi=150, bbox_inches='tight')\n",
    "        plt.close()\n",
    "        \n",
    "        # Store metadata\n",
    "        samples_data.append({\n",
    "            'filename': filename,\n",
    "            'view': view_name,\n",
    "            'laterality': lat_name,\n",
    "            'age_bin': age_bin,\n",
    "            'cancer': cancer,\n",
    "            'false_positive': false_positive\n",
    "        })\n",
    "    \n",
    "    # Save metadata\n",
    "    import pandas as pd\n",
    "    metadata_df = pd.DataFrame(samples_data)\n",
    "    metadata_df.to_csv(output_dir / 'generated_samples_metadata.csv', index=False)\n",
    "    \n",
    "    print(f\"‚úÖ Generated {num_samples} samples saved to {output_dir}\")\n",
    "    print(f\"üìä Metadata saved to {output_dir / 'generated_samples_metadata.csv'}\")\n",
    "    \n",
    "    # Backup to Drive\n",
    "    backup_to_drive(str(output_dir), 'Generated_Samples')\n",
    "\n",
    "# Generate and save samples\n",
    "save_generated_samples(20)"
   ]
